{
  "extends": [
    "react-app",
    "airbnb",
    "plugin:jsx-a11y/recommended"
  ],
  "plugins": [
    "jsx-a11y"
  ],
  "rules": {
    "semi": 0,
    "linebreak-style": 0,
    "object-curly-newline": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "operator-linebreak": [2, "after", { "overrides": { "?": "ignore", ":": "ignore"} }],
    "indent": 0
  }
}